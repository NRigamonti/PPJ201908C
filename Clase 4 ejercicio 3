<html><head>
</head>
<body>
<script>
var numero = parseInt(prompt("Ingrese un numero entero"));
var resultado= parimpar(numero);
alert("El numero "+numero+" es "+resultado);
function parimpar(numero){
	if(numero % 2 == 0){
		return "par";
	}
	else{
		return "impar";
	}
}
document.write(numero);
console.log(numero);
</script>
</body>
</html>
